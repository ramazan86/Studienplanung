<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <!-- textView_ModuleTotalView -->
    <style name="textViewModuleTotalView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginRight">@dimen/dimen_1</item>
        <item name="android:textSize">@dimen/dimen_5</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- view: verticalLine-->

    <style name="view_verticalWhiteLine">
        <item name="android:layout_width">3dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginRight">@dimen/dimen_1</item>
        <item name="android:background">@color/white</item>
    </style>







</resources>
